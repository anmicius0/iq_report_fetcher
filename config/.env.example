###############################################
# IQ Server Connection (Required)
###############################################
# API endpoint (e.g., http://your-iq-server:8070/)
IQ_SERVER_URL=http://your-iq-server:8070/

# Username
IQ_USERNAME=your_username

# Password or API token
IQ_PASSWORD=your_password_or_token

###############################################
# Organization (Optional)
###############################################
# Organization ID (empty = all accessible)
ORGANIZATION_ID=

###############################################
# Output
###############################################
# Directory for raw report files
OUTPUT_DIR=raw_reports

###############################################
# Logging
###############################################
# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO
